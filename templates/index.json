    "home_faq_shipping_QtYWGC": {
      "type": "home-faq__shipping",
      "blocks": {
        "faq_item_LGXLEb": {
          "type": "faq_item",
          "settings": {
            "question": "How long does Juicefly take to process orders?",
            "answer": "Orders placed before 4 pm on business days usually packed and dispatched same day. Order preparation may take up to 72 hours during high demand. Shipping carriers do not process orders on weekends and U.S. Federal Holidays. Orders placed on weekends or Saturday after 12PM(PST) are sent out on Mondays."
          }
        },
        "faq_item_ULqPCU": {
          "type": "faq_item",
          "settings": {
            "question": "Where Does Juicefly Ship Vapes to?",
            "answer": "Our local and national service area constantly change. Please add products to your cart and go to checkout to see most updated delivery options. Remember to login to your account before proceeding to checkout."
          }
        },
        "faq_item_BNUHjK": {
          "type": "faq_item",
          "settings": {
            "question": "Can I receive a same vape day delivery in my area?",
            "answer": "Juicefly local delivery area constantly change. Please proceed to checkout and enter your delivery address to see most updated information."
          }
        },
        "faq_item_JTd4aX": {
          "type": "faq_item",
          "settings": {
            "question": "How soon i can receive my order with local delivery?",
            "answer": "If you are in our local delivery area, you can receive your order as early as 45 minutes with 'Priority Local Delivery\". With \"Standard Local Delivery\", most orders are delivered under 2 hours."
          }
        },
        "faq_item_xJNkj3": {
          "type": "faq_item",
          "settings": {
            "question": "Can I cancel my order?",
            "answer": "Yes, you can cancel your order online. The conditions for a full refund or restocking fee depend on when you cancel your order."
          }
        },
        "faq_item_CLLEh8": {
          "type": "faq_item",
          "settings": {
            "question": "How can I track my order from Juicefly?",
            "answer": "A tracking email will be automatically generated as soon as your order fulfilled at Juicefly. If you're local delivery customer, you'll receive a tracking link through text messages to your provided phone number."
          }
        },
        "faq_item_eFVAgH": {
          "type": "faq_item",
          "settings": {
            "question": "What are the estimated transit times for different regions of the US?",
            "answer": "The Western Half of the US typically takes 1-3 business days in transit, while the Eastern Half of the US takes 3-5 business days in transit."
          }
        },
        "faq_item_tDacUM": {
          "type": "faq_item",
          "settings": {
            "question": "How are shipping costs calculated?",
            "answer": "Shipping costs are calculated based on your address or location. Please go to checkout to see most updated rates."
          }
        },
        "faq_item_yDABaz": {
          "type": "faq_item",
          "settings": {
            "question": "Is an adult signature required for vape product delivery?",
            "answer": "An Adult Signature Confirmation (21+) is required upon delivery, and the recipient must provide a valid government photo ID for age verification for their first order."
          }
        }
      },
